// Test for-each, break, continue, arr[i]=val

let mut fruits = []
push(fruits, "apple")
push(fruits, "banana")
push(fruits, "cherry")

// for-each iteration
let mut out = ""
for f in fruits
  out = out + f + " "
end
print("fruits: {out}")

// break in while
let mut n = 0.0
while n < 10.0
  if n == 3.0
    break
  end
  n = n + 1.0
end
print("break at: {n}")

// continue in while
let mut sum = 0.0
let mut i = 0.0
while i < 5.0
  i = i + 1.0
  if i == 3.0
    continue
  end
  sum = sum + i
end
print("sum skip 3: {sum}")

// array index assignment
let mut nums = []
push(nums, "10")
push(nums, "20")
push(nums, "30")
nums[1.0] = "99"
let v = nums[1.0]
print("nums[1]={v}")
